@model EKomplet.Models.Salesman

@{
    ViewData["Title"] = "Details";
}

<h1>Detaljer om @Html.DisplayFor(m => m.FirstName) @Html.DisplayFor(m => m.LastName)</h1>

<div>
    <h4>Salesman</h4>
    <hr/>
    <dl class="row">
        <dt class="col-sm-2">
            Telefonnummer
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            E-mail
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Fornavn
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            Efternavn
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.SalesmanID">Rédiger</a> |
    <a asp-action="Index">Tilbage</a>
</div>